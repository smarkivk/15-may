import java.util.*;

class abc {
public static void main(String []args)
{
    int a,b,sum;
    Scanner scanner =new Scanner (System.in);
    System.out.print("enter a  ");
    a=scanner.nextInt();
     System.out.print("enter  b ");
    b=scanner.nextInt();
    
    sum=a+b;
    System.out.println("sum"+sum);
    scanner.close();
    
}
}
